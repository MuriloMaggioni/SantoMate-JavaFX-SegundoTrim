<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="santomate1.EstoqueAddController">
    <stylesheets>
        <URL value="@CSSBotoes.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="768.0" fitWidth="1024.0" opacity="0.75" pickOnBounds="true" preserveRatio="true">
            <effect>
                <Lighting bumpInput="$null" diffuseConstant="2.0" specularConstant="0.0" specularExponent="0.0" surfaceScale="10.0">
                    <light>
                        <Light.Distant azimuth="0.0" />
                    </light>
                </Lighting>
            </effect>
            <image>
                <Image url="@imagens/erva-mate-chimarrao.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="683.0" fitWidth="284.0" layoutX="371.0" opacity="0.63" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imagens/whiteimage2.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="276.0" fitWidth="311.0" layoutX="405.0" layoutY="49.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imagens/SANTO_MATE.png" />
            </image>
        </ImageView>
        <Button fx:id="Voltar" layoutX="480.0" layoutY="624.0" mnemonicParsing="false" onAction="#voltaMenu" text="Voltar">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <ComboBox fx:id="cbxErva" layoutX="392.0" layoutY="363.0" prefHeight="34.0" prefWidth="241.0" promptText="Erva">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
        </ComboBox>
        <ComboBox fx:id="cbxQuant" layoutX="390.0" layoutY="414.0" prefHeight="34.0" prefWidth="241.0" promptText="Quantidade">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
        </ComboBox>
        <Button fx:id="add" layoutX="431.0" layoutY="471.0" mnemonicParsing="false" onAction="#addEstoque" prefHeight="34.0" prefWidth="165.0" text="Adicionar">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
