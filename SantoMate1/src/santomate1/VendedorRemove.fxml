<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="santomate1.VendedorRemoveController">
    <stylesheets>
        <URL value="@CSSBotoes.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="768.0" fitWidth="1024.0" opacity="0.75" pickOnBounds="true" preserveRatio="true">
            <effect>
                <Lighting bumpInput="$null" diffuseConstant="2.0" specularConstant="0.0" specularExponent="0.0" surfaceScale="10.0">
                    <light>
                        <Light.Distant azimuth="0.0" />
                    </light>
                </Lighting>
            </effect>
            <image>
                <Image url="@imagens/erva-mate-chimarrao.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="683.0" fitWidth="284.0" layoutX="371.0" layoutY="-1.0" opacity="0.63" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imagens/whiteimage2.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="276.0" fitWidth="311.0" layoutX="416.0" layoutY="54.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imagens/SANTO_MATE.png" />
            </image>
        </ImageView>
        <Button fx:id="voltar" layoutX="482.0" layoutY="635.0" mnemonicParsing="false" onAction="#voltarMenu" text="Voltar">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="Remove" layoutX="441.0" layoutY="445.0" mnemonicParsing="false" onAction="#demitidoOMerda" prefHeight="34.0" prefWidth="144.0" text="Remover">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Button>
      <ComboBox fx:id="Vendedor" layoutX="392.0" layoutY="386.0" prefHeight="34.0" prefWidth="241.0" promptText="Vendedor">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </ComboBox>
    </children>
</AnchorPane>
